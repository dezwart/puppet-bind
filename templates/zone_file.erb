$TTL    <%= ttl %>
@   IN  SOA <%= mname %>. <%= rname %>.<%= mname %>. (
    <%= serial %>; Serial
    <%= refresh %>; Refresh
    <%= _retry %>; Retry
    <%= expire %>; Expire
    <%= minimum %>; Minimum TTL
);

@   IN  NS  <%= mname %>.
<% if @records %><%records.each do |record| %>
<%= record %><% end %><% end %>
